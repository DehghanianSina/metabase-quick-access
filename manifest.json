{
  "manifest_version": 3,
  "name": "Metabase Quick Access",
  "version": "1.0",
  "description": "Quickly access Metabase dashboards, questions, and collections using their numbers",
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "src/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "src/icon.png",
    "48": "src/icon.png",
    "128": "src/icon.png"
  }
}
